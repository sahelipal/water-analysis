<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.waterdetails">

    <application
        android:allowBackup="true"
        android:icon="@drawable/water"
        android:label="@string/app_name"
        android:roundIcon="@drawable/water"
        android:supportsRtl="true"
        android:theme="@style/Theme.WaterDetails">
        <activity
            android:name=".about"
            android:label = "About"
            android:exported="false" />
        <activity
            android:name=".awareness"
            android:label = "Awareness"
            android:exported="false" />
        <activity
            android:name=".waterdet"
            android:label = "Water Details"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>